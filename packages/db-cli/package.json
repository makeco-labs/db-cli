{
	"name": "@makeco/db-cli",
	"version": "0.1.0",
	"author": "paulwongx",
	"repository": {
		"type": "git",
		"url": "https://github.com/makeco-labs/db-cli.git"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"db-cli": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"build": "bun clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --resolve-full-paths",
		"dev": "tsc -p tsconfig.build.json --watch",
		"dev:typecheck": "bun typecheck --watch --preserveWatchOutput",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts",
		"test": "vitest",
		"ci": "bun run lint && bun run typecheck && bun run build && bun run test --run",
		"git": "bun run ci && git add . && git commit -m",
		"postgit": "git push --all"
	},
	"bugs": {
		"url": "https://github.com/makeco-labs/db-cli/issues"
	},
	"description": "A powerful database CLI tool that extends drizzle-kit with additional commands for database management workflows",
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"homepage": "https://github.com/makeco-labs/db-cli",
	"keywords": [
		"database",
		"cli",
		"drizzle",
		"drizzle-kit",
		"migrations",
		"schema",
		"reset",
		"seed",
		"typescript"
	],
	"license": "MIT",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.3",
		"@aws-sdk/client-rds-data": "^3.556.0",
		"@cloudflare/workers-types": "^4.20230518.0",
		"@electric-sql/pglite": "^0.2.12",
		"@eslint/js": "^9.32.0",
		"@hono/node-server": "^1.9.0",
		"@hono/zod-validator": "^0.2.1",
		"@libsql/client": "^0.10.0",
		"@neondatabase/serverless": "^0.9.1",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@planetscale/database": "^1.16.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/dockerode": "^3.3.28",
		"@types/glob": "^8.1.0",
		"@types/json-diff": "^1.0.3",
		"@types/micromatch": "^4.0.9",
		"@types/minimatch": "^5.1.2",
		"@types/node": "^20.0.0",
		"@types/node-fetch": "^2.6.11",
		"@types/pg": "^8.10.7",
		"@types/prompts": "^2.4.9",
		"@types/pluralize": "^0.0.33",
		"@types/semver": "^7.5.5",
		"@types/uuid": "^9.0.8",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^8.35.0",
		"@typescript-eslint/parser": "^8.35.0",
		"@vercel/postgres": "^0.8.0",
		"ava": "^5.1.0",
		"better-sqlite3": "^11.9.1",
		"bun-types": "^0.6.6",
		"camelcase": "^7.0.1",
		"dockerode": "^4.0.6",
		"drizzle-kit": "^0.30.6",
		"env-paths": "^3.0.0",
		"esbuild-node-externals": "^1.9.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.1.3",
		"gel": "^2.0.0",
